<div class='col-sm-6 col-lg-4'>
  <div class="card">
    <div class="card-topper" style='background-image: url(<%= photo.photo_url_original %>);'>  
      <div class="card-block">
        <h4 class="card-title">
          <%= link_to photo, class: 'btn btn-read' do %>
            <p class="glyphicon glyphicon-camera"> <%=raw photo.title%></p> 
          <% end %>
        </h4>
      </div>
    </div>
  </div>
</div>